{
	"name": "any-date-parser",
	"version": "1.1.0",
	"description": "Parse a wide range of date formats including human-input dates",
	"tags": [
		"date",
		"datetime",
		"parse",
		"parser",
		"date parser",
		"parse date",
		"human"
	],
	"main": "index.js",
	"scripts": {
		"build": "npx browserify index.js -p tinyify -o dist/browser-bundle.js -t",
		"test": "node --icu-data-dir=node_modules/full-icu node_modules/jest/bin/jest.js",
		"watch": "node --icu-data-dir=node_modules/full-icu node_modules/jest/bin/jest.js --watch",
		"coverage": "node --icu-data-dir=node_modules/full-icu node_modules/jest/bin/jest.js --collectCoverage"
	},
	"browserify": {
		"transform": [
			[
				"babelify",
				{
					"presets": [
						"@babel/preset-env"
					]
				}
			]
		]
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/kensnyder/any-date-parser.git"
	},
	"author": "",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/kensnyder/any-date-parser/issues"
	},
	"homepage": "https://github.com/kensnyder/any-date-parser#readme",
	"dependencies": {},
	"devDependencies": {
		"@babel/core": "^7.11.6",
		"@babel/preset-env": "^7.11.5",
		"babelify": "^10.0.0",
		"browserify": "^17.0.0",
		"eslint": "^6.8.0",
		"full-icu": "^1.3.1",
		"jest": "^25.1.0",
		"luxon": "^1.25.0",
		"prettier": "^2.0.2",
		"tinyify": "^3.0.0"
	}
}
